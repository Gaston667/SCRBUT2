node n0 {
    type host
    network-config {
	hostname osiris
	!
	interface eth0
	 mac address 42:00:aa:00:00:00
	 ip address 192.168.1.158/24
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/8
	 ipv6 address ::1/128
	!
    }
    auto_default_routes enabled
    canvas c0
    iconcoords {96 72}
    labelcoords {96 108}
    interface-peer {eth0 n6}
}

node n1 {
    type host
    network-config {
	hostname isis
	!
	interface eth0
	 mac address 42:00:aa:00:00:01
	 ip address 192.168.1.10/24
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/8
	 ipv6 address ::1/128
	!
    }
    canvas c0
    iconcoords {576 72}
    labelcoords {576 108}
    interface-peer {eth0 n6}
    custom-enabled true
    custom-configs {
        custom-config-id default {
            custom-command /bin/sh
            config {
                ip addr add 127.0.0.1/8 dev lo0
                ip addr add 192.168.1.10/24 dev eth0
                ip -6 addr add ::1/128 dev lo0
                
                
                rpcbind
                inetd
                
            }
        }
    }
    custom-selected default
}

