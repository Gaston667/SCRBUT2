node n0 {
    type lanswitch
    network-config {
	hostname switch1
	!
    }
    canvas c0
    iconcoords {96 72}
    labelcoords {96 95}
    interface-peer {e0 n2}
    interface-peer {e1 n6}
    interface-peer {e2 n3}
}

node n1 {
    type lanswitch
    network-config {
	hostname switch2
	!
    }
    canvas c0
    iconcoords {720 72}
    labelcoords {720 95}
    interface-peer {e0 n4}
    interface-peer {e1 n6}
    interface-peer {e2 n5}
}

node n2 {
    type pc
    network-config {
	hostname pc1-1
	!
	interface eth0
	 mac address 42:00:aa:00:00:00
	 ip address 172.16.1.1/24
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/8
	 ipv6 address ::1/128
	!
    }
    auto_default_routes enabled
    canvas c0
    iconcoords {48 288}
    labelcoords {48 319}
    interface-peer {eth0 n0}
}

